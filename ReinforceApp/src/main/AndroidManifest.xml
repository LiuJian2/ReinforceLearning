<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liujian.reinforceapp">

    <application
        android:name=".ReinforceApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name="com.liujian.reinforcelearning.MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.liujian.reinforcelearning.SecondActivity"
            android:theme="@style/AppTheme" />

        <meta-data
            android:name="APPLICATION_CLASS_NAME"
            android:value="com.liujian.reinforcelearning.app.SourceApplication" />
    </application>

</manifest>